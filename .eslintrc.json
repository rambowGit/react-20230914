{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "warn",
      "react/jsx-props-no-spreading": "warn",
    }
}
